<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@CSS/Back.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Recyclage.controllers.Back">
   <children>
      <BorderPane fx:id="BorderPane" prefHeight="570.0" prefWidth="1062.0">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="Affichage" mnemonicParsing="false" onAction="#Afficher" prefHeight="187.0" prefWidth="111.0" text="Affichage" />
                  <Button fx:id="Ajouter" mnemonicParsing="false" onAction="#ajouter" prefHeight="186.0" prefWidth="185.0" text="Ajouter" />
                  <Button fx:id="Carte" mnemonicParsing="false" onAction="#carte" prefHeight="172.0" prefWidth="158.0" text="Carte" />
               </children>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
